{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["import", "typescript"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "eqeqeq": "warn",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  },
  "ignorePatterns": [".next", "node_modules"]
}
